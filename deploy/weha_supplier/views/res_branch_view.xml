<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_res_branch_form_inherit" model="ir.ui.view">
        <field name="name">res.branch.form.inherit</field>
        <field name="model">res.branch</field>
        <field name="inherit_id" ref="multi_branch_base.view_branch_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page name="page_suppliers" string="Suppliers">
                    <field name="res_branch_supplier_ids" nolabel="1">
                        <tree>
                            <field name="partner_id" />
                        </tree>
                        <form>
                            <group>
                                <field name="partner_id"/>
                            </group>
                        </form>
                    </field>
                </page>
            </xpath>
        </field>
    </record>

</odoo>
