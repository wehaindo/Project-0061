<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <!-- view common to both template and product -->
    <record id="view_inherit_product_template_form" model="ir.ui.view">
        <field name="name">inherit.product.template.form</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view" />
        <field name="arch" type="xml">
            <field name="detailed_type" position="before">
                <field name="supplier_id" />
                <field name="agreement_contract_id" domain="[('supplier_id', '=', supplier_id)]" />
            </field>
        </field>
    </record>
</odoo>