<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_template_form_view_inherit" model="ir.ui.view">
        <field name="name">product.template.form.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="weha_smart_pos_aeon_pos_data_base.product_template_form_view_inherit"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_template_data_ids']/tree[0]/field[@name='product_couchdb_sync']" position="before">
                <field name="branch_id" readonly="1"/>
            </xpath>
        </field>
    </record>

    <menuitem id="pos_config_menu_action_product_pricelist"
        parent="point_of_sale.pos_config_menu_catalog"
        action="product.product_pricelist_action2"
        groups="product.group_product_pricelist"
        sequence="20"/>
        
</odoo>
