<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_template_form_view_inherit" model="ir.ui.view">
        <field name="name">product.template.form.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page name="pos_data" string="POS Data">
                    <field name="product_template_data_ids" nolabel="1">
                        <tree editable="bottom">
                            <field name="product_template_id" invisible="1"/>
                            <field name="branch_id" readonly="0"/>
                            <field name="product_id" readonly="0" domain="[('product_tmpl_id', '=', product_template_id)]" options="{'no_create':1}"/>
                            <field name="list_price" readonly="0"/>
                            <field name="product_couchdb_sync" readonly="1"/>
                            <field name="product_json" readonly="1"/>
                            <field name="product_couchdb_id" readonly="1"/>
                            <field name="product_couchdb_rev" readonly="1"/>    
                        </tree>
                    </field>                        
                </page>
            </xpath>
        </field>
    </record>

</odoo>
