<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
        <record model="ir.module.category" id="module_category_weha_pos">
            <field name="name">WEHA POS</field>
            <field name="description">WEHA POS Backend using Odoo.</field>
            <field name="sequence">50</field>
        </record>

        <record id="group_pos_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="module_category_weha_pos"/>
        </record>

        <record id="group_pos_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="module_category_weha_pos"/>
            <field name="implied_ids" eval="[(4, ref('group_pos_user')), (4, ref('stock.group_stock_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>
    
    
</odoo>