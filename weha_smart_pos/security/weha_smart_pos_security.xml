<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="0">

    <record id="group_weha_smart_pos_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_smart_pos_category"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_weha_smart_pos_supervisor" model="res.groups">
        <field name="name">Supervisor</field>
        <field name="category_id" ref="module_smart_pos_category"/>
        <field name="implied_ids" eval="[(4, ref('group_weha_smart_pos_user'))]"/>
    </record>

    <record id="group_weha_smart_pos_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="module_smart_pos_category"/>
        <field name="implied_ids" eval="[(4, ref('group_weha_smart_pos_supervisor'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>

</data>
</odoo>
