<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="pos_product_extended_view_form" model="ir.ui.view">
        <field name="name">pos.product.extended.view.form</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='available_in_pos']" position="after">
                <field name='setu_alternative_products' widget='many2many_tags'></field>
            </xpath>
        </field>
    </record>
</odoo>
