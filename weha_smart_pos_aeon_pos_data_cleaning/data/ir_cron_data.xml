<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Cron Job to Clean Expired Pricelist Items from CouchDB -->
        <record id="ir_cron_clean_expired_pricelist_couchdb" model="ir.cron">
            <field name="name">Clean Expired Pricelist Items from CouchDB</field>
            <field name="model_id" ref="product.model_product_pricelist_item"/>
            <field name="state">code</field>
            <field name="code">model._cron_clean_expired_pricelist_from_couchdb()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
            <field name="doall" eval="False"/>
            <field name="priority">10</field>
        </record>

    </data>
</odoo>
